{
  "compilerOptions": {
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": [
      "es6",
      "es2017.object",
      "es2016.array.include",
      "dom"
    ],
    "target": "es6",
    "module": "commonjs",
    "baseUrl": "src/main",
    "paths": {
      "app/*": [
        "app/*"
      ],
      "client/*": [
        "app/client/*"
      ],
      "drafts/*": [
        "app/drafts/*"
      ],
      "claims/*": [
        "app/claims/*"
      ],
      "common/*": [
        "app/common/*"
      ],
      "fees/*": [
        "app/fees/*"
      ],
      "forms/*": [
        "app/forms/*"
      ],
      "health/*": [
        "app/health/*"
      ],
      "idam/*": [
        "app/idam/*"
      ],
      "logging/*": [
        "app/logging/*"
      ],
      "models/*": [
        "app/models/*"
      ],
      "utils/*": [
        "app/utils/*"
      ],
      "modules/*": [
        "modules/*"
      ],
      "routes/*": [
        "routes/*"
      ],
      "common/*": [
        "common/*"
      ],
      "claim/*": [
        "features/claim/*"
      ],
      "certificateOfService/*": [
        "features/certificateOfService/*"
      ],
      "dashboard/*": [
        "features/dashboard/*"
      ],
      "views/*": [
        "features/view/*"
      ],
      "services/*": [
        "services/*"
      ],
      "integration-test/*": [
        "../integration-test/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ]
  }
}
