{
  "compilerOptions": {
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "lib": [
      "es6",
      "es2017.object",
      "es2016.array.include",
      "dom"
    ],
    "target": "es6",
    "module": "commonjs",
    "baseUrl": "src",
    "paths": {
      "client/*": [
        "main/app/client/*"
      ],
      "drafts/*": [
        "main/app/drafts/*"
      ],
      "claims/*": [
        "main/app/claims/*"
      ],
      "common/*": [
        "main/app/common/*"
      ],
      "documents/*":[
        "main/app/documents/*"
      ],
      "fees/*": [
        "main/app/fees/*"
      ],
      "forms/*": [
        "main/app/forms/*"
      ],
      "health/*": [
        "main/app/health/*"
      ],
      "idam/*": [
        "main/app/idam/*"
      ],
      "logging/*": [
        "main/app/logging/*"
      ],
      "models/*": [
        "main/app/models/*"
      ],
      "pay/*": [
        "main/app/pay/*"
      ],
      "utils/*": [
        "main/app/utils/*"
      ],
      "paths": [
        "main/app/paths"
      ],
      "routes/*": [
        "main/routes/*"
      ],
      "modules/*": [
        "main/modules/*"
      ],
      "shared/*": [
        "main/common/*"
      ],
      "claim/*": [
        "main/features/claim/*"
      ],
      "certificateOfService/*": [
        "main/features/certificateOfService/*"
      ],
      "dashboard/*": [
        "main/features/dashboard/*"
      ],
      "views/*": [
        "main/features/view/*"
      ],
      "services/*": [
        "main/services/*"
      ],
      "integration-test/*": [
        "integration-test/*"
      ],
      "test/*": [
        "test/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ]
  }
}
