<form action="/legal/certificateOfService/file-upload?_csrf={{ csrf }}" novalidate method="post" enctype="multipart/form-data">
  <div class="form-group">
    <label for="files[label]"></label>
    <input type="file" id="files[label]" name="files" accept="{{ acceptedFiles }}">
  </div>

  <div class="form-group">
    {{ submitButton('Upload file') }}
  </div>
</form>
